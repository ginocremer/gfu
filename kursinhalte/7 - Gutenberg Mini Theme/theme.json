{
    "version": 2,
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "settings": {
        "layout": {
            "contentSize": "713px",
            "wideSize": "1020px"
        },
        "color": {
            "palette": [
                {
                    "slug": "red",
                    "color": "#B43C58",
                    "name": "Red"
                },
                {
                    "slug": "gray",
                    "color": "#637682",
                    "name": "Gray"
                },
                {
                    "slug": "darkgreen",
                    "color": "#6F887A",
                    "name": "Dark Green"
                },
                {
                    "slug": "lightgreen",
                    "color": "#B3C592",
                    "name": "Light Green"
                }
            ],
            "gradients": [
                {
                    "slug": "lightgreen-to-darkgreen",
                    "gradient": "linear-gradient(160deg, var(--wp--preset--color--lightgreen), var(--wp--preset--color--darkgreen))",
                    "name": "Lightgreen to Darkgreen"
                }
            ],
            "duotone": [
                {
                    "colors": [
                        "#B3C592",
                        "#6F887A"
                    ],
                    "slug": "lightgreen-and-darkgreen",
                    "name": "Lightgreen and Darkgreen"
                },
                {
                    "colors": [
                        "#1d1f35",
                        "#c7c0af"
                    ],
                    "slug": "dark-blue-and-beige",
                    "name": "Dark blue and beige"
                }
            ]
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
                    "slug": "system-fonts",
                    "name": "System Fonts"
                },
                {
                    "fontFamily": "Geneva, Tahoma, Verdana, sans-serif",
                    "slug": "geneva-verdana"
                },
                {
                    "fontFamily": "Cambria, Georgia, serif",
                    "slug": "cambria-georgia"
                }
            ]
        }
    },
    "styles": {
        "typography": {
			"fontSize": "var(--wp--preset--font-size--normal)",
			"fontFamily": "var(--wp--preset--font-family--system-fonts)",
			"lineHeight": "1.7"
		},
        "color": {
            "background": "var(--wp--preset--color--lightgreen)",
            "text": "var(--wp--preset--color--darkgreen)"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--light-red)"
                }
            }
        },
        "blocks": {
            "core/heading": {
                "color": {
                    "text": "var(--wp--preset--color--red)"
                }
            },
            "core/list": {
                "color": {
                    "text": "var(--wp--preset--color--darkgreen)"
                }
            }
        }
    }
}